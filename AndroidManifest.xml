        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.cdio.successor.ui.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.cdio.successor.ui.LearnModeActivity"
            android:label="@string/title_activity_learn_mode" >
        </activity>
        <activity
            android:name="com.cdio.successor.ui.JieQiActivity"
            android:label="@string/title_activity_jie_qi" >
        </activity>
    </application>

</manifest>
